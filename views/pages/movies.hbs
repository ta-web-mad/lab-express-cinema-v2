<div class="container">
    <h1>Ironhack Cinema</h1>
    <h3>Click on the movie to check the showtimes!</h3>
    <div class="row">
        {{#each movies}}
        <div class="col-4 centered">
            <img src="{{this.image}}" alt="{{this.title}}">
            <h5>{{this.title}}</h5>
            <a href="/movie/{{this.id}}">See more!</a>
        </div>
        {{/each}}

    </div>
</div>